function ExecuteEvent(func)
	pcall(func)
end

function KickPlayer(Reason)
	game.Players.LocalPlayer:Kick(Reason)
end

function Clear()
	ScriptBox.Text = ""
end

function SendNotification(text, seconds)
	local UI = Instance.new("Frame")
	local UIBorder = Instance.new("Frame")
	local NotificationText = Instance.new("TextLabel")

	UI.Name = "UI"
	UI.Parent = NotificationUI
	UI.BackgroundColor3 = Color3.fromRGB(53, 53, 53)
	UI.BorderColor3 = Color3.fromRGB(0, 0, 0)
	UI.BorderSizePixel = 0
	UI.Position = UDim2.new(-0.149118349, 0, 0.0399999991, 0)
	UI.Size = UDim2.new(0, 130, 0, 23)

	UIBorder.Name = "UIBorder"
	UIBorder.Parent = UI
	UIBorder.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	UIBorder.BorderColor3 = Color3.fromRGB(0, 0, 0)
	UIBorder.BorderSizePixel = 0
	UIBorder.Position = UDim2.new(-0.00296443049, 0, 0.0869565234, 0)
	UIBorder.Size = UDim2.new(0, 130, 0, -2)

	NotificationText.Name = "NotificationText"
	NotificationText.Parent = UI
	NotificationText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	NotificationText.BackgroundTransparency = 1.000
	NotificationText.BorderColor3 = Color3.fromRGB(0, 0, 0)
	NotificationText.BorderSizePixel = 0
	NotificationText.Position = UDim2.new(0, 0, 0.0869565234, 0)
	NotificationText.Size = UDim2.new(0, 130, 0, 21)
	NotificationText.Font = Enum.Font.FredokaOne
	NotificationText.Text = text
	NotificationText.TextColor3 = Color3.fromRGB(255, 255, 255)
	NotificationText.TextScaled = true
	NotificationText.TextSize = 14.000
	NotificationText.TextWrapped = true
	wait(seconds)
	local TweenService = game:GetService("TweenService")
	local Tween = TweenService:Create(UI, TweenInfo.new(1), {BackgroundTransparency = 1})
	
	Tween:Play()
end

function TweenCloseButton()
	local TweenService = game:GetService("TweenService")
	local Tween = TweenService:Create(CloseButton, TweenInfo.new(1), {ImageColor3 = Color3.fromRGB(255, 24, 7)})

	Tween:Play()
end

function unTweenCloseButton()
	local TweenService = game:GetService("TweenService")
	local Tween = TweenService:Create(CloseButton, TweenInfo.new(1), {ImageColor3 = Color3.fromRGB(255, 255, 255)})

	Tween:Play()
end

ExecuteButton.MouseButton1Click:Connect(function()
	local ScriptEx = ScriptBox.Text
ExecuteEvent(ScriptEx)
SendNotification("Executed!", 3)
end)

ClearButton.MouseButton1Click:Connect(function()
	Clear()
	SendNotification("Clear!", 3)
end)

CloseButton.MouseEnter:Connect(function()
	TweenCloseButton()
end)

CloseButton.MouseLeave:Connect(function()
	unTweenCloseButton()
end)

